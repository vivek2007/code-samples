<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
<% if @page_title %>
<title>Uncharted - <%= @page_title %></title>
<% else %>
<title>Uncharted - Explore. Live. Feel. </title>
<% end %>


<%= stylesheet_link_tag 'style_menu','style11','menu' %>
<%= javascript_include_tag :defaults %>

<%= javascript_include_tag 'login_dropdown' %>  <!--login drop down and search div-->

<% if params[:controller] == "account" && ( params[:action] == 'register' || params[:action] == 'create' ) %>
<%= javascript_include_tag 'validations' %>  <!--registration page validations-->
<% end %>

<%= javascript_include_tag 'field_sets' %>  <!--share module vlidations -->
<%= javascript_include_tag 'home' %>

<!--google map API key-->
<script src="http://maps.google.com/maps?file=api&v=2&markers=21.796875,31.952162,blues|21.796875,31.952162,tinygreen&key=ABQIAAAALHXVqR3ivZP4lPuASHGUbRRxI6gS_yiE9yY2PwSnUHXXLgJkihRW61pIbuWHVb-9kU-NwqDke2W2iw" 
    type="text/javascript">
</script>
	
<script src="http://www.google.com/uds/api?file=uds.js&v=1.0&key=ABQIAAAALHXVqR3ivZP4lPuASHGUbRRxI6gS_yiE9yY2PwSnUHXXLgJkihRW61pIbuWHVb-9kU-NwqDke2W2iw" 
    type="text/javascript">
</script>
<!--google map API key-->



<script>
window.onload=function()
{
loadmap();
tab_image(document.getElementById('image_tab').value);

<% if session[:user_id].blank? %>
first_div();
<% end %>

<% if flash[:login_falied] %>
logindiv(0);
<% end %>

}
</script>
  
 
 
 
</head>
<body>
<!--main-->
<center>
<%= hidden_field_tag 'image_tab', 'account', :id => 'image_tab' %>


<!--google ad div-->
<div style="width:95%; height:auto; margin:20px 0px 0px 20px;  padding:0px;" align="center" >
		<div style="width:728px; height:90px; margin:20px 0px 0px 20px;" align="center">
		<% if !@header_adv.blank? %>
			<%= @header_adv %>
			<% end %>				
		</div>
</div>

<!--google ad div end -->

<div id="main">

<!--header-->
<div id="header">
<div class="friends_wid">
<img src="/images/home/top_left.jpg"  alt=""/></div>
<div class="top">
<div class="top1">
<!--Logo div -->
       <div class="shared_go_head">
	   <a href="<%= home_url %>" title="uncharted"> <img src="/images/home/logo.gif" alt="uncharted" /></a> 
	   </div>
 <!--Logo div end-->
<div class="toplink">
						<div class="go_nyros" onmouseover="show_this('go');" onmouseout="hide_this('go');"><img src="/images/home/go_hover.jpg" alt="go" id="go_img"/></div>
						<div class="see1" onmouseover="show_this('see');" onmouseout="hide_this('see');"><img src="/images/home/see_hover.jpg" alt="see" id="see_img"/></div>
						<div class="see1" onmouseover="show_this('do');" onmouseout="hide_this('do');"><img src="/images/home/do_hover.jpg" alt="do" id="do_img"/></div>				
						</div>	
</div>
		
<div class="search_box">							
	<div class="shared_go_search" onmouseover="show_this('share');" onmouseout="hide_this('share');">
	<img src="/images/home/share.jpg" alt="share" id="share_img"/>
	</div>					
	<div class="shared_go_home"> <img src="/images/home/back_strip.jpg" alt="" /></div>
    </div>	
	</div>
	<div class="shared_go_right"><img src="/images/home/top_right.jpg" alt="" /></div>
    </div>

<!--header-div-end-->





<!--content-->

<!--menu-->
<div class="content">

	<div id="default">
	   <%= render :partial => '/shared/default_menu'%>
	</div>
	
	<div id="go" style="display:none;">
    <%= render :partial => 'shared/go_menu'%>
	</div>
	
	<div id="see" style="display:none;">
	<%= render :partial => 'shared/see_menu'%>
   </div>
   
   	<div id="do" style="display:none;">
	<%= render :partial => 'shared/do_menu'%>
   </div>
	
	 <div id="share" style="display:none;">
	 <%= render :partial => 'shared/share_menu'%>
	 </div>
	
<div  style="clear:both;"></div>
<!--menu-div-end-->



<!--middle-->


   	<% if params[:controller] == "account" && params[:action] == 'index' %>
	<div class="content_center">
  			<div style="width:850px;float:left; margin:0px; padding:0px;">
    <% else %>
	<div class="content_new">
   			 <div style="width:810px;float:left; margin:0px; padding:0px; ">
    <% end %>
	
	
    <div style="float:left; width:820px;">
	 <%= yield %>
    </div>
	
</div>

    <% if params[:controller] == "account" && params[:action]== 'index' %>
   			 <div style="width:200px; height:553px; float:left;">
    <% else %>
      		<div class="right_1">
   		    <div class="inner_div">
            <div class="cornerbg"></div>
    <% end %>
	
	<% if params[:controller] == "account" && params[:action]== 'index' %>
    <%= render :partial => "/shared/home_right_block"%>
	<% else %>
	 <%= render :partial => "/shared/see_right_block"%>
	<% end %>
	
	
    <% if params[:controller] == "account" && params[:action]== 'index' %>
        </div>
    <% else %>
       </div>
       </div>
    <% end %>
  </div>
  <!--middle end-->
  
  <div style="clear:both;"></div>
  
	<% if params[:controller] == "account" && params[:action] == 'index' %>  
  
    <% else %>
  <div style="width:100%; background-color:#E3DDC7; overflow:hidden;">
    <!--<div style="float:left; text-align:left; width:845px; height:56px;">
	 <img src="/images/home/register_new.jpg" border="0" alt="" width="845" height="56"/>
	 </div>-->
	 <div style="float:left; text-align:left; width:845px; margin:0; padding:0; background:url(/images/home/Green_Footer.jpg) no-repeat; height:56px;">
	<!-- <img src="/images/home/register_new.jpg" border="0" alt="" width="845" height="56"/>-->
	
	 </div>
  </div>
 <% end %> 
  
  
  
</div>
<!--content-div-end-->





<div style="clear:both;"></div>

<div style="background-color:#444444;">
  <%= render :partial => 'layouts/footer'%>
</div>



<!--main-div-end-->
</div>
</center>
<%= render :partial => 'shared/google_analytics_code' %>
</body>
</html>
